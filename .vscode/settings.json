{
  "files.associations": {
    "*.cpp": "cpp",
    "*.h": "cpp",
    "iostream": "cpp",
    "__bit_reference": "cpp",
    "__locale": "cpp",
    "__verbose_abort": "cpp",
    "array": "cpp",
    "bitset": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "cstdarg": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "initializer_list": "cpp",
    "ios": "cpp",
    "iosfwd": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "locale": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "optional": "cpp",
    "print": "cpp",
    "queue": "cpp",
    "ratio": "cpp",
    "stack": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "typeinfo": "cpp",
    "variant": "cpp",
    "vector": "cpp",
    "memory": "cpp",
    "algorithm": "cpp"
  },
  "C_Cpp.default.compilerPath": "/usr/bin/clang++",
  "C_Cpp.default.includePath": [
    "/Library/Developer/CommandLineTools/usr/include/c++/v1",
    "/usr/local/include",
    "/usr/include",
    "${workspaceFolder}"
  ],
  "C_Cpp.default.cppStandard": "c++17",
  "code-runner.executorMap": {
    "cpp": "clang++ -std=c++17 -stdlib=libc++ $fullFileName -o $fileNameWithoutExt && $dir/$fileNameWithoutExt"
  },
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "C_Cpp.errorSquiggles": "disabled"
}
